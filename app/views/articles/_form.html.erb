<%= simple_form_for(@article) do |f| %>
  <%= f.error_notification %>

  <div class="">
    <%= f.input :title %>
    </div>
    <div class="">
    <%= f.input :content, as: :bootsy %>
    </div>
     <div class="">
    <%= f.input :photos, as: :attachinary %>
  </div>

  <div class="form-actions">
    <%= f.button :submit , "CREATE Article", class: "navbar-wagon-button btn-submit", style: 'margin-left: 0px; padding: 5px;' %>
  </div>
<% end %>
